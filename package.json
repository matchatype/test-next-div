{
  "name": "test-next-div",
  "version": "1.0.0",
  "description": "test-next-div",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next \"src/app\"",
    "preserve": "npm run build:public && npm run build:static && npm run build:functions && npm run build:app && npm run copy:deps && npm run install:deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "predeploy": "npm run build:public && npm run build:static && npm run build:functions && npm run build:app && npm run copy:deps",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
    "build:app": "next build \"src/app/\"",
    "build:functions": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build:public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build:static": "cpx \"src/app/static/**/*.*\" \"dist/public/static\" -C",
    "copy:deps": "cpx \"*{package.json,package-lock.json,yarn.lock,credentials/server.json}\" \"dist/functions\" -C",
    "install:deps": "cd \"dist/functions\" && npm i",
    "validate": "lint-staged"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@sentry/browser": "^5.6.3",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "baseui": "^9.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "firebase": "^6.6.1",
    "firebase-admin": "^8.5.0",
    "firebase-functions": "^3.2.0",
    "helmet": "^3.21.0",
    "hoist-non-react-statics": "^3.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "^11.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "session-file-store": "^1.3.1",
    "styletron-engine-atomic": "^1.4.1",
    "styletron-react": "^5.2.1",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "babel-eslint": "^10.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^7.3.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
}
